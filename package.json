{
  "name": "fang",
  "version": "1.0.0",
  "description": "房字典",
  "author": "savoygu <savoygu@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "linters": {
      "{pages,components,layouts,middleware,plugins,store}/**/*.{vue,js}": [
        "npm run lint",
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": []
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.2",
    "nuxt-sass-resources-loader": "^2.0.3",
    "sass-bem": "^2.6.5",
    "sass-loader": "^7.0.3"
  }
}
